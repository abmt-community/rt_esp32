[
	{"key": "idf_path", "form": "text", "lbl": "IDF Path", "default": "~/esp-idf"},
	{"key": "heavy_lifting", "form": "checkbox", "lbl": "Download IDF when not found", "default": true },
	{"key": "sdkcfg", "form": "select", "lbl": "Device", "default": "c3", "options": { "c3": "ESP32_C3", "c6": "ESP32_C6", "esp32": "ESP32","test": "test" } },
	{"key": "serial_port", "form": "text", "lbl": "Serial Port", "default": "/dev/ttyACM0"},
	{"key": "serial_device", "form": "select", "lbl": "Serial Device", "default": "0", "options": { "0": "USB/UART0", "1": "UART0", "2": "No Communication"} },
	{"key": "serial_baudrate", "form": "hidden", "lbl": "Serial Baud Rate", "default": 921600 },
	{"key": "enable_wifi", "form": "checkbox", "lbl": "Enable Wifi", "default": true },
	{"key": "wifi_cfg_h", "form": "text", "lbl": "Path To Wifi Config Header", "default": "./wifi_cfg.h"},
	{"key": "mdns_name", "form": "text", "lbl": "MDNS Host Name", "default": "esp32" },
	{"key": "wifi_sleep", "form": "select", "lbl": "WiFi Power Save Mode", "default": "0", "options": { "0": "none", "1": "min", "2": "max / no_itwt", "3": "max / itwt 50ms", "4": "max / itwt 25ms"} },
	{"key": "wifi_interval", "form": "text", "lbl": "WiFi Listen Interval [ms]", "default": "100"},
	{"key": "sleep", "form": "checkbox", "lbl": "Sleep On Idle", "default": false },
	{"key": "skip", "form": "checkbox", "lbl": "Skip Build, Only Connect", "default": false }
]
